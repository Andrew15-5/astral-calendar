    "@types/hbs": "^4.0.1",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "pretty": "^2.0.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hbs": "^4.2.0"
